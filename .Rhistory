params.set <- cbind(
threshold = lhs[ ,1] * (threshold.max-threshold.min) + threshold.min,
expansion = lhs[ ,2] * (expansion.max-expansion.min) + expansion.min,
inflation = lhs[ ,3]*(inflation.max-inflation.min) + inflation.min
)
